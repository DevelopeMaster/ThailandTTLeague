<body>
    <header class="header" id="scroll">
       <!-- from js -->
    </header>
    <div class="navigate">
        <div class="container">
            <nav aria-label="breadcrumb">
                <ol id="breadcrumb" class="navigate_breadcrumb">
                    <!-- from js -->
                </ol>
            </nav>
        </div>
    </div>



    <section class="filterTournaments">
        <div class="container">
            <div class="contentWrapper">
                <h2>FIND A Tournament</h2>
                <div class="filterTournaments_filter">
                    <div class="filterTournaments_wrapper">
                        <div class="dropdownForm datesForm">
                            <label for="dateFrom">Tournament date</label>
                            <input type="text" name="dateFrom" id="dateFromInput" placeholder="Date from">
                            <input type="text" name="dateUntil" id="dateUntilInput" placeholder="Date until">
                        </div>
                        <div class="dropdownForm">
                            <label for="club">Club</label>
                            <input type="text" name="club" id="clubInput" placeholder="Club name">
                            <div id="clubDropdown" class="dropdown-content"></div>
                        </div>
                    </div>
                    <div class="filterTournaments_wrapper">
                        <div class="dropdownForm">
                            <label for="city">City</label>
                            <input type="text" name="city" id="cityInput" placeholder="City">
                            <div id="cityDropdown" class="dropdown-content"></div>
                        </div>
                        <button id="filterTournaments_btnSearch" class="header_btn-sign btnSbmt filterCoaches_sbmt" type="submit" fdprocessedid="c3chv">Search</button>
                    </div>
                </div>
                
            </div>
        </div>
    </section>

    <section class="tournaments">
        <div class="container">
          <div class="contentWrapper">
            <h2>Upcoming tournaments</h2>
            <div class="upcommingTable">
              <div class="upcommingTable_header">
                  <span class="tournament_time">Time</span>
                  <span class="tournament_club">Club</span>
                  <span class="tournament_restrict">Rating limit</span>
                  <span class="tournament_rating">Rating</span>
                  <span class="tournament_players">Players</span>
              </div>
              <div id="upcoming-tournaments" class="upcommingTable_content">
                <div class="upcommingTable_weekday">Среда 29.05.2024</div>
                <div class="upcommingTable_tournament">
                  <div class="cell tournament_time">11:00</div>
                  <div class="cell tournament_club">
                    <div class="clubLogo" style="background-image: url('/icons/clubslogo/HarricaneTT-logo.png'); background-position: 50%; background-size: cover; background-repeat: no-repeat;" ></div>
                    <span>HurricaneTT</span>
                  </div>
                  <div class="cell tournament_restrict">
                    <div class="restrictionStatus">
                      <div class="restriction">277</div>
                    </div>
                  </div>
                  <div class="cell tournament_rating">368</div>
                  <div class="cell tournament_players">
                    <img src="/icons/user.svg" alt="person">
                    <span>8</span>
                  </div>
                </div>
                <!-- <a class="upcommingTable_btn" href="#">
                  See more
                </a> -->
              </div>
            </div>
          </div>
        </div>
    </section>



    <section class="tournaments">
        <div class="container">
          <div class="contentWrapper">
            <h2>Last tournaments</h2>
            <div class="lastTournamentsTable">
              <div class="lastTournamentsTable_header">
                  <span class="tournament_time">Time</span>
                  <span class="tournament_club">Club</span>
                  <span class="tournament_restrict">Rating limit</span>
                  <span class="tournament_rating">Rating</span>
                  <span class="tournament_players">Players</span>
              </div>
              <div id="past-tournaments" class="lastTournamentsTable_content">
                <div class="lastTournamentsTable_weekday">Среда 29.05.2024</div>
                <div class="lastTournamentsTable_tournament">
                  <div class="cell tournament_time">11:00</div>
                  <div class="cell tournament_club">
                    <div class="clubLogo" style="background-image: url('/icons/clubslogo/HarricaneTT-logo.png'); background-position: 50%; background-size: cover; background-repeat: no-repeat;" ></div>
                    <span>HurricaneTT</span>
                  </div>
                  <div class="cell tournament_restrict">
                    <div class="restrictionStatus">
                      <div class="restriction">277</div>
                    </div>
                  </div>
                  <div class="cell tournament_rating">368</div>
                  <div class="cell tournament_players">
                    <img src="/icons/user.svg" alt="person">
                    <span>8</span>
                  </div>
                </div>
                <a class="lastTournamentsTable_btn" href="#">
                  See more
                </a>
              </div>
            </div>
          </div>
        </div>
    </section>



    <footer class="footer">
        <!-- from js -->
      </footer>
      
      <div class="adv">
        <!-- add from DB -->
      </div>
      
    
    
      <!-- Модальное окно -->
      <div id="myModal" class="modal">    
          <div class="modal-content">
            <!-- add from DB -->
          </div>
      </div>
