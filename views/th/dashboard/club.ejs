    <section class="club sectionSchedule">
        <div class="container">
            <div class="contentWrapper">
              <h2>Schedule</h2>
                <form class="secondForm">
                    <div class="editschedule_formContainer schedule-container">
                        <table class="schedule-form schedule-table" >  
                        
                        </table>
                    </div>
                </form>
  
            </div>
        </div>
    </section>

    <section class="clubLocation">
        <div class="container">
            <div class="contentWrapper">
                <h2>แผนที่สโมสร</h2>
                <div id="map"></div>
            </div>
        </div>
    </section>

    <section class="tournaments">
        <div class="container">
          <div class="contentWrapper">
            <h2>การแข่งขันที่กำลังจะจัดขึ้น</h2>
            <div class="upcommingTable">
              <div class="upcommingTable_header">
                  <span class="tournament_time">วันที่</span>
                  <span class="tournament_club">สโมสร</span>
                  <span class="tournament_restrict">คะแนนขั้นต่ำ</span>
                  <span class="tournament_rating">คะแนนเฉลี่ย</span>
                  <span class="tournament_players">ผู้เล่น</span>
              </div>
              <div id="upcoming-tournaments" class="upcommingTable_content">
                
                
              </div>
            </div>
          </div>
        </div>
    </section>

    <section class="clubStatistics">
        <div class="container">
            <div class="contentWrapper">
                <h2>สถิติ</h2>
                <!-- from js -->
                <div class="tournamentsTable">
                    <h3>การแข่งขัน</h3>
                    <div class="tournamentsTable_header">
                        <span class="player_number">№</span>
                        <span class="player_player">Player name</span>
                        <span class="player_tournaments">Tour.</span>
                        <span class="player_rating">Rating</span>
                    </div>
                    <div id="tournamentsTable" class="tournamentsTable_content">
                        
                    </div>
                    <a class="tournamentsTable_btn" href="#">ดูเพิ่มเติม</a>
                </div>

                <div class="victoriesTable">
                    <h3>ชัยชนะ</h3>
                    <div class="victoriesTable_header">
                        <span class="player_number">№</span>
                        <span class="player_player">Player name</span>
                        <span class="player_tournaments">Tour.</span>
                        <span class="player_rating">Rating</span>
                    </div>
                    <div id="victoriesTable" class="victoriesTable_content">
                        
                    </div>
                    <a class="victoriesTable_btn" href="#">ดูเพิ่มเติม</a>
                </div>

            </div>
        </div>
    </section>

    <section class="clubWinners">
        <div class="container">
            <div class="contentWrapper">
                <h2>ผู้ชนะ</h2>
                <!-- from js -->
                <div class="winnersTable">
                    <div class="winnersTable_header">
                        <span class="player_number">№</span>
                        <span class="player_player">Player name</span>
                        <span class="player_places">
                            <img src="/icons/1st-medal.svg" alt="gold medal">
                            <img src="/icons/2st-medal.svg" alt="silver medal">
                            <img src="/icons/3st-medal.svg" alt="bronze medal">
                        </span>
                        <span class="player_tournaments">Tour.</span>
                        <span class="player_rating">Rating</span>
                    </div>
                    <div id="winnersTable" class="tournamentsTable_content">
                        
                    </div>
                    <a class="winnersTable_btn" href="#">ดูเพิ่มเติม</a>
                </div>

            </div>
        </div>
    </section>

    <section class="clubPhotos">
        <div class="container">
            <div id="clubsPhotoContainer" class="contentWrapper">
                <h2>รูปสโมสร</h2>
                

            </div>
        </div>
    </section>



